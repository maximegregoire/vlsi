#include "system.h"
#include "io.h"


#include <stdio.h>

#define INTCTL 	0x0
#define TCTL 	0x1
#define T0CNT	0x2
#define T1CNT	0x3
#define T0CMP	0x4
#define T1CMP	0x5
#define GP0		0x6
#define GP1		0x7

int main()
{

  // Set up Control registers


	// Enable T0INTEN(0) active high
	// Enable T1INTEN(1) active high
	// Enable AVINTDIS(5) active low
	IOWR_32DIRECT(REGFILE_0_BASE + INTCTL, 0, 0x00000003)


  return 0;
}


